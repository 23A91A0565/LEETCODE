class Solution {
    public int maximumCount(int[] nums) {
        int n1=0,n2=0;
        for(int i=0;i<nums.length;i++)
        {
            if(nums[i]<0)
            {
                n1+=1;
            }
            else if(nums[i]>0)
            {
                n2+=1;
            }
        }
        return Math.max(n1,n2);
    }
}
